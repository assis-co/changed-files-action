{
  "preset": "eslint",
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    "master",
    "next",
    "next-major",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "repositoryUrl": "https://github.com/assis-co/changed-files-action.git",
  "plugins": ["@semantic-release/commit-analyzer", "@semantic-release/github"]
}
